{
  "name": "@nuxtjs/strapi",
  "version": "0.1.12",
  "description": "Strapi module for Nuxt",
  "repository": "nuxt-community/strapi-module",
  "license": "MIT",
  "sideEffects": false,
  "main": "lib/module.js",
  "typings": "./types/index.d.ts",
  "files": [
    "lib",
    "types"
  ],
  "scripts": {
    "build": "siroc build && yarn build:runtime",
    "build:runtime": "tsc -p tsconfig.runtime.json && copyfiles \"src/runtime/**/*\" -e \"**/*.ts\" -u 2 lib/runtime",
    "dev": "yarn nuxt-ts example",
    "docs": "nuxt docs",
    "lint": "eslint --ext .ts --ext .vue .",
    "release": "yarn test && standard-version && yarn build && git push --follow-tags && npm publish",
    "test": "yarn lint && jest"
  },
  "dependencies": {
    "@nuxt/http": "^0.6.2",
    "cookie-universal-nuxt": "^2.1.4",
    "defu": "^3.2.2",
    "destr": "^1.0.1",
    "hookable": "^4.3.1",
    "ms": "^2.1.3",
    "requrl": "^3.0.2",
    "ufo": "^0.5.4"
  },
  "devDependencies": {
    "@babel/core": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@commitlint/cli": "^11.0.0",
    "@commitlint/config-conventional": "^11.0.0",
    "@nuxt/test-utils": "^0.1.2",
    "@nuxt/types": "^2.14.12",
    "@nuxt/typescript-build": "^2.0.3",
    "@nuxt/typescript-runtime": "^2.0.0",
    "@nuxtjs/eslint-config": "^5.0.0",
    "@nuxtjs/eslint-config-typescript": "^5.0.0",
    "@nuxtjs/module-test-utils": "latest",
    "@types/jest": "^26.0.20",
    "babel-eslint": "latest",
    "babel-jest": "^26.6.3",
    "copyfiles": "^2.4.1",
    "eslint": "^7.17.0",
    "husky": "^4.3.7",
    "jest": "^26.6.3",
    "nuxt": "2.14.12",
    "siroc": "^0.6.0",
    "standard-version": "^9.1.0",
    "ts-loader": "^8.0.14",
    "typescript": "^4.1.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
